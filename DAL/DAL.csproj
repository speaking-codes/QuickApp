  </ItemGroup>
  <ItemGroup Label="RegressionPredictionModel">
    <None Include="RegressionPredictionModel.consumption.cs">
      <DependentUpon>RegressionPredictionModel.mbconfig</DependentUpon>
    </None>
    <None Include="RegressionPredictionModel.training.cs">
      <DependentUpon>RegressionPredictionModel.mbconfig</DependentUpon>
    </None>
    <None Include="RegressionPredictionModel.zip">
      <DependentUpon>RegressionPredictionModel.mbconfig</DependentUpon>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\MachineLearningModel\MachineLearningModel.csproj" />
  </ItemGroup>
