<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Version>7.6.1</Version>
    <Description>Data Access Layer for the Quick Application template</Description>
    <Copyright>Copyright © 2023 www.ebenmonney.com</Copyright>
    <PackageProjectUrl>https://www.ebenmonney.com/quickapp</PackageProjectUrl>
    <Company>EBENMONNEY</Company>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Models\AgeRatingCoefficient.cs" />
    <Compile Remove="Models\CustomerInsuranceCategoryPolicyRating.cs" />
    <Compile Remove="Models\JobRalRatingCoefficient.cs" />
    <Compile Remove="Models\Temp.cs" />
    <Compile Remove="Repositories\AgeRatingCoefficientRepository.cs" />
    <Compile Remove="Repositories\CustomerInsuranceCategoryPolicyRatingRepository.cs" />
    <Compile Remove="Repositories\Interfaces\IAgeRatingCoefficientRepository.cs" />
    <Compile Remove="Repositories\Interfaces\ICustomerInsuranceCategoryPolicyRatingRepository.cs" />
    <Compile Remove="Repositories\Interfaces\IHealthcareProvisionTypeRepository.cs" />
    <Compile Remove="Repositories\Interfaces\IJobRalRatingCoefficientRepository.cs" />
    <Compile Remove="Repositories\JobRalRatingCoefficientRepository.cs" />
  </ItemGroup>
  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="7.0.13" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="7.0.13">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.ML" Version="1.7.1" />
    <PackageReference Include="Microsoft.ML.LightGbm" Version="1.7.1" />
    <PackageReference Include="MongoDB.Driver" Version="2.23.1" />
