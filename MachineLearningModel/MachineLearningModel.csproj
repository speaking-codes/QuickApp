<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.ML" Version="1.7.1" />
    <PackageReference Include="Microsoft.ML.FastTree" Version="1.7.1" />
    <PackageReference Include="Microsoft.ML.Recommender" Version="0.19.1" />
  </ItemGroup>
  <ItemGroup Label="ClassificationLearningModel">
    <None Include="ClassificationLearningModel.consumption.cs">
      <DependentUpon>ClassificationLearningModel.mbconfig</DependentUpon>
    </None>
    <None Include="ClassificationLearningModel.training.cs">
      <DependentUpon>ClassificationLearningModel.mbconfig</DependentUpon>
    </None>
    <None Include="ClassificationLearningModel.zip">
      <DependentUpon>ClassificationLearningModel.mbconfig</DependentUpon>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>